{
  "ServiceID": "bestpracticesdemo",
  "ApplicationInsights": {
    "InstrumentationKey": "AppInsightKeyForEnv"
  },
  "Logging": {
    "WriteDiagnostics": true,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    },
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "UseDebugSqlLogger": true,
    "EnableAppInsights": true
  },
  "CORS": {
    "Origins": "*"
  },
  "AllowedHosts": "*"
}
